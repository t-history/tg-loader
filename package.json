{
  "name": "tg-history",
  "version": "1.0.0",
  "description": "This npm package allows for regular creation of dumps of all Telegram chats using the Telegram Bot API, storing them in JSON format, and enabling automatic dumps at specified intervals, useful for archiving or analyzing messages, or for creating backups in case of data loss",
  "scripts": {
    "start:dev": "npx nodemon",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf ./build && tsc",
    "start": "npm run build && node build/index.js",
    "lint": "eslint . --ext .ts"
  },
  "author": "Mois",
  "license": "ISC",
  "dependencies": {
    "@types/deep-diff": "^1.0.2",
    "bullmq": "^3.12.0",
    "chalk": "^5.2.0",
    "deep-diff": "^1.0.2",
    "dotenv": "^16.0.3",
    "ioredis": "^5.3.1",
    "mongodb": "^5.3.0",
    "tdl": "^7.2.0",
    "tdl-tdlib-addon": "^1.2.2",
    "tdlib-types": "^0.1001008.1"
  },
  "devDependencies": {
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.15.10",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.22",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
